&GLOBAL
  PROGRAM_NAME ATOM
&END GLOBAL
&ATOM
  ELEMENT C

  RUN_TYPE ENERGY

  ELECTRON_CONFIGURATION [He] 2s2 2p2
  MAX_ANGULAR_MOMENTUM 1

  &METHOD
     METHOD_TYPE  KOHN-SHAM
     RELATIVISTIC DKH(3)
     &XC
       &XC_FUNCTIONAL PBE
       &END XC_FUNCTIONAL
     &END XC
  &END METHOD

  &OPTIMIZATION
    EPS_SCF 1.e-8
  &END 

  &PRINT
     &ADMM
        &ADMM_BASIS
           BASIS_SET_FILE_NAME EMSL_BASIS_SETS
           BASIS_TYPE CONTRACTED_GTO 
           BASIS_SET 3-21Gx
        &END ADMM_BASIS
     &END ADMM
  &END PRINT

  &AE_BASIS
     BASIS_SET_FILE_NAME EMSL_BASIS_SETS
     BASIS_TYPE CONTRACTED_GTO 
     BASIS_SET aug-cc-pVTZ
  &END AE_BASIS

&END ATOM
