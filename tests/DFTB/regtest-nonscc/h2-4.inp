#CPQA INCLUDE DFTB/nonscc/nonscc_parameter
#CPQA INCLUDE uff_table
#CPQA INCLUDE DFTB/nonscc/hh
&FORCE_EVAL
  &DFT
    &QS
      METHOD DFTB
      &DFTB
        SELF_CONSISTENT    F
        &PARAMETER
          PARAM_FILE_PATH  DFTB/nonscc
          PARAM_FILE_NAME  nonscc_parameter
        &END PARAMETER
      &END DFTB
    &END QS
    &SCF
      SCF_GUESS NONE
      &MIXING
          METHOD DIRECT_P_MIXING
          ALPHA   1.
      &END
    &END SCF
  &END DFT
  &SUBSYS
    &CELL
      ABC 20.0 20.0 20.0
      PERIODIC NONE
    &END CELL
    &COORD
     H     0.000000    0.000000    0.000000   
     H     0.000000    0.700000    0.000000  
    &END COORD
  &END SUBSYS
&END FORCE_EVAL
&GLOBAL
  PROJECT h2-4
  RUN_TYPE MD
  PRINT_LEVEL LOW
&END GLOBAL
&MOTION
  &MD
    ENSEMBLE NVE
    STEPS 5
    TIMESTEP 0.5
    TEMPERATURE 330.0
  &END MD
&END MOTION
