&GLOBAL
  PROJECT fli_nopol_small2
  PREFERRED_FFT_LIBRARY  FFTW
  PRINT_LEVEL LOW
  PROGRAM_NAME CP2K
  RUN_TYPE ENERGY
&END GLOBAL
&FORCE_EVAL
 METHOD FIST
 &MM
   &FORCEFIELD
     &SPLINE
       R0_NB 0.1
       EMAX_SPLINE 20.0
     &END SPLINE
     &NONBONDED
        &BMHFTD
          atoms F F
          A 181.8373
          B 4.2833
          C 0.329381
          D 0.922357
          BD 3.59048
          RCUT 9.36643
        &END BMHFTD
        &BMHFTD
          atoms Li F
          A 20.4158478
          B 3.87854
          C 0.0
          D 0.0
          BD 0.0
          RCUT 9.36643
        &END BMHFTD
        &BMHFTD
          atoms Li Li
          A 195.910854
          B 8.03542
          C 0.0
          D 0.0
          BD 0.0
          RCUT 9.36643
        &END BMHFTD
     &END NONBONDED
     &CHARGE
       atom F
       CHARGE 0.0000
     &END CHARGE
     &CHARGE
       atom Li
       CHARGE 0.000
     &END CHARGE
   &END FORCEFIELD
   &POISSON
     PERIODIC NONE
     POISSON_SOLVER ANALYTIC
     &EWALD
       GMAX 7
     &END EWALD
    &END POISSON
  &END MM
   &SUBSYS
    &CELL
      ABC 6.0 6.0 6.0
      PERIODIC NONE
    &END CELL
    &COORD
     F     0.0 0.0 0.0
     Li    0.0 0.0 2.0
    &END COORD
    &KIND F
      ELEMENT F
      MASS    18.99840
    &END KIND
    &KIND Li
      ELEMENT Li
      MASS    6.941
    &END KIND
   &END SUBSYS
 &END FORCE_EVAL
