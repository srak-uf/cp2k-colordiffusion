&FORCE_EVAL
  METHOD FIST
  &MM
    &FORCEFIELD
      ! PARM91
      ! VDW_SCALE14 0.5
      ! EI_SCALE14  0.5
      ! PARM94 and Following
      VDW_SCALE14 0.5 
      EI_SCALE14  0.8333333
      parm_file_name ../sample_pot/h2o2_amber.pot
      parmtype CHM
      SHIFT_CUTOFF .false.
    &END FORCEFIELD
    &POISSON
      &EWALD
        EWALD_TYPE ewald
        ALPHA .36
        GMAX 51
      &END EWALD
    &END POISSON
  &END MM
  &SUBSYS
    &CELL
      ABC 20.0 20.0 20.0
    &END CELL
    &TOPOLOGY
      CONN_FILE_NAME ../sample_psf/h2o2_amber.psf
      CONNECTIVITY UPSF
      COORD_FILE_NAME ../sample_pdb/h2o2_amber.pdb
      COORDINATE PDB
    &END TOPOLOGY
    &PRINT
      &TOPOLOGY_INFO
        UTIL_INFO
      &END
    &END
  &END SUBSYS
&END FORCE_EVAL
&GLOBAL
  PROJECT h2o2_amber
  PRINT_LEVEL DEBUG
  RUN_TYPE md
&END GLOBAL
&MOTION
  &MD
    ENSEMBLE NVE
    STEPS 1
    TIMESTEP 1.0
    TEMPERATURE 0.0
  &END MD
&END MOTION
