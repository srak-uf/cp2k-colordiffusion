&FORCE_EVAL
  METHOD FIST
  &MM
    &FORCEFIELD
      parm_file_name ../sample_pot/water.pot
      parmtype CHM
      &CHARGE
        ATOM OT
        CHARGE -0.8476
      &END CHARGE
      &CHARGE
        ATOM HT
        CHARGE 0.4238
      &END CHARGE
    &END FORCEFIELD
    &POISSON
      &EWALD
        EWALD_TYPE pme
        ALPHA .44
        NS_MAX 25
      &END EWALD
    &END POISSON
  &END MM
  &SUBSYS
    &CELL
      ABC 24.955 24.955 24.955
    &END CELL
    &TOPOLOGY
      COORD_FILE_NAME ../sample_pdb/water_2s.pdb
      COORDINATE pdb
      &GENERATE
       REORDER
      &END
    &END TOPOLOGY
    &PRINT
       &TOPOLOGY_INFO
         UTIL_INFO
       &END
    &END
  &END SUBSYS
&END FORCE_EVAL
&GLOBAL
  PROJECT water_reord
  RUN_TYPE MD
&END GLOBAL
&MOTION
  &MD
    ENSEMBLE NVE
    STEPS 10
    TIMESTEP 2.5
    TEMPERATURE 298
  &END MD
&END MOTION
