&FORCE_EVAL
  METHOD Quickstep
  &DFT
    BASIS_SET_FILE_NAME GTH_BASIS_SETS
    POTENTIAL_FILE_NAME POTENTIAL
    &MGRID
      CUTOFF 200
      REL_CUTOFF  40
      NGRIDS 4
    &END MGRID
    &QS
    &END QS
    &SCF
      SCF_GUESS ATOMIC
      EPS_SCF 1.00E-05
      MAX_SCF 8
      &OT
#        ALGORITHM IRAC
        MINIMIZER DIIS
#       N_DIIS 7
        PRECONDITIONER FULL_ALL
      &END OT
      &OUTER_SCF
         EPS_SCF 1.00E-05
         MAX_SCF 40
      &END
    &END SCF
    &XC
      &XC_FUNCTIONAL PADE
      &END XC_FUNCTIONAL
    &END XC
  &END DFT
  &SUBSYS
    &CELL
      ABC 13.8 13.8 13.8
    &END CELL
    &COORD
F                 0.0000        13.8000         0.0000
H                 0.2619        12.8748         0.2559
F                 3.4500         6.9000         0.0000
H                 3.7119         5.9748         0.2559
F                 6.9000        13.8000         0.0000
H                 7.1619        12.8748         0.2559
F                10.3500         6.9000         0.0000
H                10.6119         5.9748         0.2559
F                 2.0000        13.8000         6.9000
H                 2.2619        12.8748         7.1559
    &END COORD
    &KIND H
      BASIS_SET DZVP-GTH
      POTENTIAL GTH-BLYP-q1
    &END KIND
    &KIND F
      BASIS_SET DZVP-GTH
      POTENTIAL GTH-BLYP-q7
    &END KIND
    &TOPOLOGY
      &MOL_SET
        &MOLECULE
          NMOL 5
          CONN_FILE_NAME topology_atoms_hf.psf
        &END
      &END
      CONNECTIVITY MOL_SET
    &END TOPOLOGY
  &END SUBSYS
&END FORCE_EVAL
&GLOBAL
  PROJECT HF_MC
  RUN_TYPE MC
  PRINT_LEVEL LOW
&END GLOBAL
&MOTION
  &MC
    ENSEMBLE traditional
    TEMPERATURE 350.0
    IPRINT 1
    LBIAS no
    LSTOP yes
    NMOVES 1
    RCLUS 3.5 
    NSTEP 5
    PRESSURE 1.013
    RESTART no
    RESTART_FILE_NAME mc_restart_1
    ETA 0.0
    VIRIAL_TEMPS 300.0
    &MOVE_PROBABILITIES
      PMSWAP 0.0
      PMTRAION 0.0
      PMTRANS 0.0
      PMCLTRANS 1.0
      PMVOLUME 0.0
      PMHMC 0.0
      PMAVBMC 0.0
      &MOL_PROBABILITIES
        PMAVBMC_MOL 1.0
        PMSWAP_MOL 1.0
        PMTRAION_MOL 1.0
        PMTRANS_MOL 1.0
        PMROT_MOL 1.0
      &END MOL_PROBABILITIES
      &BOX_PROBABILITIES
        PMVOL_BOX 1.0
        PMHMC_BOX 1.0
        PMCLUS_BOX 1.0
      &END BOX_PROBABILITIES
    &END MOVE_PROBABILITIES
    &MOVE_UPDATES
      IUPTRANS 50
      IUPVOLUME 50
    &END MOVE_UPDATES
    &MAX_DISPLACEMENTS
      &MOL_DISPLACEMENTS
        RMDIHEDRAL 3.0
        RMANGLE 3.0
        RMBOND 0.074
        RMROT 26.0
        RMTRANS 4.38
      &END MOL_DISPLACEMENTS
      &BOX_DISPLACEMENTS
        RMVOLUME 100.5
        RMCLTRANS 1.0
      &END BOX_DISPLACEMENTS
    &END MAX_DISPLACEMENTS
    &AVBMC
      AVBMC_ATOM 1
      AVBMC_RMIN 1.0
      AVBMC_RMAX 5.0
      PBIAS 0.5
    &END AVBMC
  &END MC
&END MOTION
