&FORCE_EVAL
  METHOD Quickstep
  &DFT
    BASIS_SET_FILE_NAME GTH_BASIS_SETS
    BASIS_SET_FILE_NAME BASIS_ADMM
    POTENTIAL_FILE_NAME GTH_POTENTIALS
    &MGRID
      CUTOFF 100
      REL_CUTOFF 30
    &END MGRID
    &QS
      METHOD GPW
      EPS_PGF_ORB 1.0E-10
      EPS_FILTER_MATRIX 0.0e0
    &END QS
    &AUXILIARY_DENSITY_MATRIX_METHOD
      METHOD CHARGE_CONSTRAINED_PROJECTION
      ADMM_PURIFICATION_METHOD NONE 
      EXCH_SCALING_MODEL MERLOT   
    &END
    &SCF
      EPS_SCF 1.0E-4
      SCF_GUESS ATOMIC
      MAX_SCF 20
      &OT ON
         PRECONDITIONER FULL_SINGLE_INVERSE
         MINIMIZER DIIS
      &END
    &END SCF
    &XC
      &XC_FUNCTIONAL
        &PBE
          SCALE_X 0.75
          SCALE_C 1.0
        &END
      &END XC_FUNCTIONAL
      &HF
        &SCREENING
          EPS_SCHWARZ 1.0E-6
          SCREEN_ON_INITIAL_P FALSE
        &END
        &MEMORY
          MAX_MEMORY 900
          EPS_STORAGE_SCALING 0.1
        &END
        &INTERACTION_POTENTIAL
          POTENTIAL_TYPE TRUNCATED
          CUTOFF_RADIUS 0.5
          T_C_G_DATA t_c_g.dat
        &END
        FRACTION 0.25
      &END
    &END XC
  &END DFT
  &SUBSYS
    &CELL
      ABC 5.0 5.0 5.0
    &END CELL
    &COORD
C 	0.0000 	0.0000 	0.0000
H 	0.6297 	0.6297 	0.6297
H 	-0.6297 	-0.6297 	0.6297
H 	-0.6297 	0.6297 	-0.6297
H 	0.6297 	-0.6297 	-0.6297
    &END COORD
    &KIND H
      BASIS_SET DZVP-GTH
      BASIS_SET AUX_FIT FIT3
      POTENTIAL GTH-PBE-q1
    &END KIND
    &KIND C
      BASIS_SET DZVP-GTH
      BASIS_SET AUX_FIT FIT3
      POTENTIAL GTH-PBE-q4
     &END KIND
  &END SUBSYS
  STRESS_TENSOR NUMERICAL 
  &PRINT
    &STRESS_TENSOR
    &END STRESS_TENSOR
  &END PRINT
&END FORCE_EVAL
&GLOBAL
  PROJECT CH4-ADMMS-ST-Numerical 
  PRINT_LEVEL MEDIUM
  RUN_TYPE ENERGY_FORCE
&END GLOBAL
