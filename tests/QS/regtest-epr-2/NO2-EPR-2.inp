                 &XC_FUNCTIONAL
                    &XALPHA
                    &END XALPHA
                 &END XC_FUNCTIONAL
               &END XC
            &END G_TENSOR
         &END PRINT
       &END
    &END
  &END
  &SUBSYS
    &CELL
      ABC 5.0 5.0 5.0 
    &END CELL
    &COORD
  N 0.000000  0.000000  0.000000
  O 0.000000  0.000000  1.207900
  O 0.872252  0.000000 -0.835583
    &END COORD
    &KIND N
      BASIS_SET 3-21G*
      POTENTIAL ALL
    &END KIND
    &KIND O
      BASIS_SET 3-21G*
      POTENTIAL ALL
    &END KIND
  &END SUBSYS
&END FORCE_EVAL
