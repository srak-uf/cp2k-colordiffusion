&FORCE_EVAL
  METHOD QS
  &DFT
    BASIS_SET_FILE_NAME ALL_BASIS_SETS
    POTENTIAL_FILE_NAME POTENTIAL
    &MGRID
      CUTOFF 200
    &END MGRID
    &QS
       METHOD GAPW
       EPS_DEFAULT 1.0E-12
       EPS_PGF_ORB 1.0E-15
    &END QS
    &SCF
      EPS_SCF 3.0E-8
      MAX_SCF 1
      SCF_GUESS ATOMIC
      &OT
         MINIMIZER CG
         PRECONDITIONER FULL_ALL
         LINE_SEARCH 3PNT
         ENERGY_GAP 0.5
      &END
      &PRINT
        &RESTART OFF
        &END
      &END
    &END SCF
    &XC
      &XC_FUNCTIONAL PADE
      &END XC_FUNCTIONAL
    &END XC
  &END DFT
  &SUBSYS
    &CELL
      ABC 8 8 8
      PERIODIC NONE
    &END CELL
    &COORD
     Fe   4. 4. 4.
    &END COORD
    &KIND Fe
      BASIS_SET 6-31G*
      POTENTIAL ALL
    &END KIND
  &END SUBSYS
&END FORCE_EVAL
&GLOBAL
  PROJECT Fe
  PRINT_LEVEL MEDIUM
  RUN_TYPE ENERGY
  FLUSH_SHOULD_FLUSH
&END GLOBAL
