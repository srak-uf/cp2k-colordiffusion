&FORCE_EVAL
  METHOD Quickstep
  &DFT
    BASIS_SET_FILE_NAME ALL_BASIS_SETS
    POTENTIAL_FILE_NAME POTENTIAL
    &MGRID
      CUTOFF 200
      NGRIDS 1
      REL_CUTOFF 80
    &END MGRID
    &QS
      ALPHA0_H 10
      EPS_GVG 1.0E-8
      EPS_PGF_ORB 1.0E-8
      EPSFIT 1.E-4
      EPSISO 1.0E-12
      EPSSVD 0.0
      EPSRHO0 1.E-8
      LMAXN0 2
      LMAXN1 6
      METHOD GAPW
      QUADRATURE GC_LOG
    &END QS
    &SCF
      EPS_DIIS 0.4
      EPS_SCF 1.0E-8
      MAX_SCF 50
      SCF_GUESS ATOMIC
    &END SCF
    &XC
      DENSITY_CUTOFF 1.0e-11
      &XC_FUNCTIONAL TPSS
      &END XC_FUNCTIONAL
    &END XC
  &END DFT
  &SUBSYS
    &CELL
      ABC 8.0 8.0 8.0
    &END CELL
    &COORD
   F 0.0 0.0 0.0
   H 0.0 0.0 0.917
    &END COORD
    &KIND F
      BASIS_SET 6-311ppG3f2d
      LEBEDEV_GRID 70
      POTENTIAL ALL
      RADIAL_GRID 100
    &END KIND
    &KIND H
      BASIS_SET 6-311ppG3f2d
      LEBEDEV_GRID 60
      POTENTIAL ALL
      RADIAL_GRID 100
    &END KIND
  &END SUBSYS
&END FORCE_EVAL
&GLOBAL
  PROJECT HF_gapw
  RUN_TYPE WFN_OPT
  PRINT_LEVEL LOW   
&END GLOBAL
