&FORCE_EVAL
  METHOD Quickstep
  &DFT
    LSD
    BASIS_SET_FILE_NAME ALL_BASIS_SETS
    POTENTIAL_FILE_NAME POTENTIAL
    &MGRID
      CUTOFF 80
      NGRIDS 1
      REL_CUTOFF 30
    &END MGRID
    &QS
      ALPHA0_H 10
      EPS_GVG 1.0E-8
      EPS_PGF_ORB 1.0E-4
      EPSFIT 1.E-4
      EPSISO 1.0E-12
      EPSSVD 0.0
      EPSRHO0 1.E-8
      LMAXN0 4
      LMAXN1 6
      METHOD GAPW
      QUADRATURE GC_LOG
     
    &END QS
    &SCF
      EPS_DIIS 0.4
      EPS_SCF 3.0E-6
      MAX_SCF 2
       
      SCF_GUESS ATOMIC
    &END SCF
    &XC
      DENSITY_CUTOFF 1.0e-11
      &XC_FUNCTIONAL Blyp
      &END XC_FUNCTIONAL
    &END XC
  &END DFT
  &SUBSYS
    &CELL
      ABC 12.0 12.0 12.0
    &END CELL
    &COORD
   O 0.0     0.0 0.0
   F 1.35427 0.0 0.0
    &END COORD
    &KIND O
      BASIS_SET 6-311ppG3f2d
      LEBEDEV_GRID 30
      POTENTIAL ALL
      RADIAL_GRID 30
    &END KIND
    &KIND F
      BASIS_SET 6-311ppG3f2d
      LEBEDEV_GRID 30
      POTENTIAL ALL
      RADIAL_GRID 30
    &END KIND
  &END SUBSYS
&END FORCE_EVAL
