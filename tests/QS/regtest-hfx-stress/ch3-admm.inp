&FORCE_EVAL
  METHOD Quickstep
  &DFT
    BASIS_SET_FILE_NAME GTH_BASIS_SETS
    BASIS_SET_FILE_NAME BASIS_ADMM
    POTENTIAL_FILE_NAME GTH_POTENTIALS
    LSD
    &MGRID
      CUTOFF 100
      REL_CUTOFF 30
    &END MGRID
    &QS
      METHOD GPW
      EPS_PGF_ORB 1.0E-12
   EPS_FILTER_MATRIX 0.0e0
    &END QS
    &AUXILIARY_DENSITY_MATRIX_METHOD
      METHOD BASIS_PROJECTION
      ADMM_PURIFICATION_METHOD MO_DIAG
    &END
    &SCF
      EPS_SCF 1.0E-6
      SCF_GUESS ATOMIC
      MAX_SCF 20
      &OT ON
      &END
      &OUTER_SCF
        EPS_SCF 1.0E-6
        MAX_SCF 20
      &END
    &END SCF
    &XC
      &XC_FUNCTIONAL
        &PBE
          SCALE_X 0.75
          SCALE_C 1.0
        &END
      &END XC_FUNCTIONAL
      &HF
        &SCREENING
          EPS_SCHWARZ 1.0E-6
          SCREEN_ON_INITIAL_P FALSE
        &END
        &MEMORY
          MAX_MEMORY 900
          EPS_STORAGE_SCALING 0.1
        &END
        &INTERACTION_POTENTIAL
          POTENTIAL_TYPE TRUNCATED
          CUTOFF_RADIUS 1.0
          T_C_G_DATA t_c_g.dat
        &END
        FRACTION 0.25
      &END
    &END XC
  &END DFT
  &SUBSYS
    &CELL
      ABC 4.0 4.0 4.0
      PERIODIC NONE
    &END CELL
    &COORD
C       0.0000 	0.0000 	0.0000
H       0.0000 	1.0728 	0.0000
H       0.9291 	-0.5364 0.0000
H      -0.9291 -0.5364 0.0000
    &END COORD
    &KIND H
      BASIS_SET DZVP-GTH
      BASIS_SET AUX_FIT cFIT3
      POTENTIAL GTH-PBE-q1
    &END KIND
    &KIND C
      BASIS_SET DZVP-GTH
      BASIS_SET AUX_FIT cFIT3
      POTENTIAL GTH-PBE-q4
     &END KIND
  &END SUBSYS
  STRESS_TENSOR ANALYTICAL
  &PRINT
    &STRESS_TENSOR
    &END STRESS_TENSOR
  &END PRINT

&END FORCE_EVAL
&GLOBAL
  PROJECT ch3-admm
  PRINT_LEVEL MEDIUM
  RUN_TYPE ENERGY_FORCE
&END GLOBAL
