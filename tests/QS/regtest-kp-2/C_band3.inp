&FORCE_EVAL
  &DFT
    BASIS_SET_FILE_NAME GTH_BASIS_SETS
    POTENTIAL_FILE_NAME POTENTIAL
    &MGRID
      CUTOFF 120
      REL_CUTOFF 30
    &END MGRID
    &QS
      METHOD GPW
      EPS_DEFAULT 1.0E-10
    &END QS
    &SCF
      EPS_SCF 1.0E-6
      SCF_GUESS ATOMIC 
      MAX_SCF  8
      &OT
         MINIMIZER DIIS
         PRECONDITIONER FULL_ALL
      &END
      &OUTER_SCF
         EPS_SCF 1.0E-6
         MAX_SCF  20
      &END
    &END SCF
    &XC
      &XC_FUNCTIONAL PADE
      &END XC_FUNCTIONAL
    &END XC
    &PRINT
       &BAND_STRUCTURE
          FILE_NAME xx.bs
          &KPOINT_SET
             UNITS CART_BOHR
             NPOINTS     10
             SPECIAL_POINT  0.0 0.0 0.0
             SPECIAL_POINT  1.0 0.0 0.0
          &END
       &END BAND_STRUCTURE
    &END PRINT
  &END DFT
  &SUBSYS
    &TOPOLOGY
      MULTIPLE_UNIT_CELL 1 1 1
    &END
    &CELL
      ABC 3.56683 3.56683 3.56683
      MULTIPLE_UNIT_CELL 1 1 1
    &END CELL
    &COORD
     SCALED
     C     0.000000    0.000000    0.000000
     C     0.500000    0.500000    0.000000
     C     0.500000    0.000000    0.500000
     C     0.000000    0.500000    0.500000
     C     0.250000    0.250000    0.250000
     C     0.250000    0.750000    0.750000
     C     0.750000    0.250000    0.750000
     C     0.750000    0.750000    0.250000
    &END COORD
    &KIND C
      BASIS_SET DZVP-GTH
      POTENTIAL GTH-PADE-q4
    &END KIND
  &END SUBSYS
&END FORCE_EVAL
&GLOBAL
  PROJECT C
  PRINT_LEVEL LOW
&END GLOBAL
