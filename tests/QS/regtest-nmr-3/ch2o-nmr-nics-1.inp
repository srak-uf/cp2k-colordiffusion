&GLOBAL
  PROJECT ch2o-nmr-nics-1
  PRINT_LEVEL LOW
  RUN_TYPE LINEAR_RESPONSE
&END GLOBAL
&FORCE_EVAL
  &DFT
    BASIS_SET_FILE_NAME GTH_BASIS_SETS
    POTENTIAL_FILE_NAME GTH_POTENTIALS
    &MGRID
      CUTOFF 150
    &END MGRID
    &QS
    &END QS
    &SCF
      SCF_GUESS ATOMIC
    &END SCF
    &XC
      &XC_FUNCTIONAL BLYP
      &END XC_FUNCTIONAL
    &END XC
  &END DFT
  &PROPERTIES
    &LINRES
       &LOCALIZE
       &END
       MAX_ITER 100
       PRECONDITIONER FULL_ALL
       &CURRENT
          GAUGE R_AND_STEP_FUNCTION
       &END CURRENT
       &NMR
          NICS T
          NICS_FILE_NAME ./nics_ch2o
       &END
    &END
  &END
  &SUBSYS
    &CELL
      ABC 5.0 5.0 5.0
    &END CELL
    &COORD
     O     0.051368    0.000000    0.000000     
     C     1.278612    0.000000    0.000000    
     H     1.870460    0.939607    0.000000   
     H     1.870460   -0.939607    0.000000  
    &END COORD
    &KIND H
      BASIS_SET DZVP-GTH
      POTENTIAL GTH-BLYP-q1
    &END KIND
    &KIND O
      BASIS_SET DZVP-GTH
      POTENTIAL GTH-BLYP-q6
    &END KIND
    &KIND C
      BASIS_SET DZVP-GTH
      POTENTIAL GTH-BLYP-q4
    &END KIND
  &END SUBSYS
&END FORCE_EVAL
