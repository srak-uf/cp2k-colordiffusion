          SELECTED_STATES_ON_ATOM_LIST 4 5 6
          SELECTED_STATES_ATOM_RADIUS 1.0
          GAUGE ATOM
          ORBITAL_CENTER WANNIER
       &END CURRENT
       &NMR
       &END
    &END
  &END
  &SUBSYS
    &CELL
      ABC 5.0 5.0 5.0
    &END CELL
    &COORD
 O     0.000000     0.000000     0.000000
 H     0.000000     0.000000     0.950000
 H     0.895669     0.000000    -0.316667
 O    -1.885618     0.000000    -2.333333
 H    -2.084656    -0.895669    -2.579630
 H    -2.350039     0.597112    -2.908025
    &END COORD
    &KIND H
      BASIS_SET 3-21G*
      POTENTIAL ALL
    &END KIND
    &KIND O
      BASIS_SET 3-21G*
      POTENTIAL ALL
    &END KIND
  &END SUBSYS
&END FORCE_EVAL
