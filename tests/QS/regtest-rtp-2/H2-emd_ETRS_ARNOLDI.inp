&FORCE_EVAL
  METHOD QUICKSTEP
  &DFT
    &REAL_TIME_PROPAGATION
       MAX_ITER 10 
       MAT_EXP ARNOLDI
       EXP_ACCURACY 1.0E-9
       EPS_ITER 1.0E-3
       PROPAGATOR ETRS
       INITIAL_WFN SCF_WFN
    &END
    &EFIELD
      ENVELOP CONSTANT
      INTENSITY 5.0E10
      POLARISATION 0.859311 0.511454 0
      WAVELENGTH 900
      &CONSTANT_ENV
       START_STEP 1
       END_STEP 3
      &END
     &END
    BASIS_SET_FILE_NAME BASIS_SET
    POTENTIAL_FILE_NAME POTENTIAL
    &MGRID
      CUTOFF 100
    &END MGRID
    &QS
      EPS_DEFAULT 1.0E-10
      MAP_CONSISTENT
    &END QS
    &SCF
      MAX_SCF 20
      EPS_SCF 1.0E-6
      SCF_GUESS ATOMIC
      &OUTER_SCF
        EPS_SCF 1.0E-6
      &END
      &OT ON
        ROTATION
      &END OT
    &END SCF
    &XC
      &XC_FUNCTIONAL PADE
      &END XC_FUNCTIONAL
    &END XC
  &END DFT
  &SUBSYS
    &CELL
      ABC 6.0 6.0 6.0
    &END CELL
    &COORD
      O   0.000000    0.000000   -0.065587 H2O
      H   0.000000   -0.757136    0.520545 H2O
      H   0.000000    0.757136    0.520545 H2O
    &END COORD
    &KIND H
      BASIS_SET DZVP-GTH-PBE
      POTENTIAL GTH-PBE-q1
    &END KIND
    &KIND O
      BASIS_SET DZVP-GTH-PBE
      POTENTIAL GTH-PBE-q6
    &END KIND
  &END SUBSYS
&END FORCE_EVAL
&GLOBAL
  PROJECT H2-emd_ETRS_ARNOLDI
 RUN_TYPE EHRENFEST_DYN
!  RUN_TYPE ENERGY 
  PRINT_LEVEL LOW
&END GLOBAL
&MOTION
  &MD
    ENSEMBLE NVE
    STEPS 5
    TIMESTEP [au_t] 0.5
    TEMPERATURE 100.0
  &END MD
    &PRINT
       &TRAJECTORY
          &EACH
             MD 1
          &END
       &END
    &END
&END MOTION
