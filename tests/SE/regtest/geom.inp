&GLOBAL
  PROJECT geom
  PRINT_LEVEL                      LOW
  RUN_TYPE                         GEO_OPT
&END GLOBAL
&MOTION
  &GEO_OPT
    OPTIMIZER                      BFGS
    MAX_ITER                       5
    &PRINT
      &PROGRAM_RUN_INFO
      &END PROGRAM_RUN_INFO
    &END PRINT
  &END GEO_OPT
  &PRINT
    &TRAJECTORY SILENT
      FILENAME                     =OMP_OPT.xyz
    &END TRAJECTORY
    &VELOCITIES SILENT
      FILENAME                     =OMP_OPT.vel
    &END VELOCITIES
    &FORCES SILENT
      FILENAME                     =OMP_OPT.for
    &END FORCES
    &RESTART_HISTORY OFF
    &END RESTART_HISTORY
    &RESTART                       SILENT
      &EACH
        MD                         20
      &END
