# two potential (Nested Monte Carlo) on 3 temperatures and volume move and swaps

&GLOBAL
  PROJECT H2O_TMC
  PROGRAM TMC
  RUN_TYPE TMC
  PRINT_LEVEL LOW
  #TRACE
  WALLTIME 00:01:30 
&END GLOBAL
&MOTION
  &TMC
      GROUP_ENERGY_SIZE 1
      GROUP_ENERGY_NR 1
      GROUP_CC_SIZE 0
      NUM_MC_ELEM 20
      ENERGY_FILE_NAME H2O_ice.inp
      NR_TEMPERATURE 3
      TEMPERATURE 270 330
      &NMC_MOVES
        NMC_FILE_NAME H2O_ice_2.inp
        NR_NMC_STEPS 2
        &MOVE_TYPE      ATOM_TRANS
          SIZE          0.1
          PROB          1
          INIT_ACC_PROB 0.2
        &END
        &MOVE_TYPE      VOL_MOVE
          SIZE          0.01
          PROB          8
        &END
      &END NMC_MOVES
      &MOVE_TYPE      PT_SWAP
        PROB          5
      &END
      PRESSURE 0.001
      NUM_MV_ELEM_IN_CELL 1
      RND_DETERMINISTIC 42
      INFO_OUT_STEP_SIZE 10
      RESTART_OUT 0
      PRINT_TEST_OUTPUT
  &END TMC
&END MOTION
