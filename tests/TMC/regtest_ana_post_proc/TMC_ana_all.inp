&GLOBAL
  PROJECT H2O_TMC
  PROGRAM MC_ANALYSIS
  RUN_TYPE TMC
  PRINT_LEVEL LOW
&END GLOBAL
&MOTION
  &TMC
    PRINT_TEST_OUTPUT
    &TMC_ANALYSIS_FILES
      NR_TEMPERATURE    3
      TEMPERATURE       300 460
      FORCE_ENV_FILE    H2O.inp
      DENSITY           2 1 1
      G_R
      CLASSICAL_DIPOLE_MOMENTS
      &CHARGE
        ATOM O
        CHARGE -0.834
      &END CHARGE
      &CHARGE
        ATOM H
        CHARGE 0.417
      &END CHARGE
      DEVIATION
    &END TMC_ANALYSIS_FILES
  &END TMC
&END MOTION
